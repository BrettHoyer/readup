#preview-pane 
  position absolute 
  right 0 
  height 100%
  width 25% 
  margin-right 50px 
  border 1px dashed black
  overflow scroll
  display none
  #close-preview
    position absolute
    top 10px
    right 10px
    background blue
    width 10px
    height 10px

.site-thumbnail-container
  width 200px 
  height 200px 
  overflow hidden

.site-thumbnail
  width 200px
  cursor pointer

  
