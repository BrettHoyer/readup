@font-face {
  font-family: 'readup-icons';
  src:url('../lib/readup-icons87652/fonts/readup-icons.eot');
  src:url('../lib/readup-icons87652/fonts/readup-icons.eot?#iefix') format('embedded-opentype'),
    url('../lib/readup-icons87652/fonts/readup-icons.woff') format('woff'),
    url('../lib/readup-icons87652/fonts/readup-icons.ttf') format('truetype'),
    url('../lib/readup-icons87652/fonts/readup-icons.svg#readup-icons') format('svg');
  font-weight: normal;
  font-style: normal;
}

.icon-readup:before, .icon-eye:before, .icon-github:before, .icon-checkmark:before, .icon-checkmark:after {
  font-family: 'readup-icons';
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
}
.icon-readup:before {
  content: "\e000";
}
.icon-eye:before {
  content: "\e001";
}
.icon-github:before {
  content: "\e002";
}
.icon-checkmark:before {
  content: "\e003";
}
.icon-checkmark:after {
  content: "\e003";
}