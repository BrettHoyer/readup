@font-face {
  font-family: 'icomoon';
  src:url('../lib/icomoon76310/fonts/icomoon.eot');
  src:url('../lib/icomoon76310/fonts/icomoon.eot?#iefix') format('embedded-opentype'),
    url('../lib/icomoon76310/fonts/icomoon.woff') format('woff'),
    url('../lib/icomoon76310/fonts/icomoon.ttf') format('truetype'),
    url('../lib/icomoon76310/fonts/icomoon.svg#icomoon') format('svg');
  font-weight: normal;
  font-style: normal;
}

.icon-bullhorn, .icon-book {
  font-family: 'icomoon';
  speak: none;
  font-style: normal;
  font-weight: normal;
  font-variant: normal;
  text-transform: none;
  line-height: 1;
  -webkit-font-smoothing: antialiased;
}
.icon-bullhorn:before {
  content: "\e000";
}
.icon-book:before {
  content: "\e001";
}